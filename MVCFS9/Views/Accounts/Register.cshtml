@model User

@{
    Layout = "_AccountsLayout";
}

Register

<form asp-controller="Accounts" asp-action="Register">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label asp-for="UserName"></label>
            <input class="form-control" placeholder="User Name" asp-for="UserName">
        </div>

        <div class="form-group col-md-6">
            <label asp-for="UserEmail"></label>
            <input class="form-control" id="inputEmail4" placeholder="Email" asp-for="UserEmail">
        </div>

        <div class="form-group col-md-6">
            <label asp-for="UserPassword"></label>
            <input class="form-control" placeholder="Password" asp-for="UserPassword" >
        </div>

        <div class="form-group col-md-6">
            <label asp-for="UserPhone"></label>
            <input class="form-control" placeholder="phone number" asp-for="UserPhone"  >
        </div>


        <div class="form-group col-md-6">
            <label asp-for="CreatedAt"></label>
            <input class="form-control" asp-for="CreatedAt">
        </div>


    </div>
    <br />
    <button type="submit" class="btn btn-primary">Register</button>



</form>